MAPQUEST_API_KEY=your-api-key-here
TOMTOM_API_KEY==your-api-key-here
GOOGLE_MAPS_API_KEY=your-api-key-here

# API Configuration
TOMTOM_API_KEY=your_actual_key_here
GOOGLE_MAPS_API_KEY=your_actual_key_here

# Set TomTom as preferred provider (most generous free tier)
MAP_PROVIDER=tomtom

# Optimized Rate Limiting for Free Tiers
# TomTom: 2,500 calls/day allows ~60 calls/minute in bursts
MAX_API_CALLS_PER_MINUTE=60
API_CACHE_TTL_SECONDS=1800

# Enable aggressive caching to reduce API usage
ENABLE_REQUEST_BATCHING=true
AGGRESSIVE_CACHING=true

# Optional: Simulation defaults
DEFAULT_DRIVERS=200
DEFAULT_DURATION_HOURS=4.0
